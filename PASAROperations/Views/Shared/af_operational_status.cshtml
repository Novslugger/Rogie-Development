<script type="text/javascript">


    $(document).ready(function () {

        $('#newaf_shiftdate_select1').change(function () {
            if ($(this).on('change')) {
                $('#newaf_shift_btn1').removeAttr('disabled');
                //if ($(this).on('click')) {
                //$('select').prop("disabled", false);
                //}
            } else {
                $('#newaf_shift_btn1').attr('disabled', 'disabled');
            }
        });

        $("#newaf_shiftdate1").datepicker({
            showOn: 'button',
            buttonText: 'Show Date',
            buttonImageOnly: true,
            buttonImage: '../Images/calendar.png',
            dateFormat: 'mm/dd/yy',
            constrainInput: true,
            onSelect: function (date) {

                assign_fsfe_prodid();
            }
        });

        $('#ui-datepicker-div').css('display', 'none');
        $('#newaf_shiftdate1').datepicker('setDate', 'today');
       
        $(".ui-datepicker-trigger").mouseover(function () {


            $(this).css('cursor', 'pointer');

        });



    });

    //load_af_page();




    $('#newaf_manpower_table select').each(function () {
        $(this).live('change', function () {
            var id = $(this).attr('id');
            var val = $(this).val();
            //alert('.');
            if (val == "") {
                //alert('a');
                $('.newaf_manpower_table_1').find('select[id="' + id + 'a"]').prop('disabled', false);
                $('.newaf_manpower_table_1').find('select[id="' + id + 'b"]').prop('disabled', false);
            }
            else {
                //alert('b');
                $('.newaf_manpower_table_1').find('select[id="' + id + 'a"]').prop('disabled', true);
                $('.newaf_manpower_table_1').find('select[id="' + id + 'b"]').prop('disabled', true);
            }
        });
    });

