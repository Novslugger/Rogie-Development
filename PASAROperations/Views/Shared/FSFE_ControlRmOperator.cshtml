<script type="text/javascript">


    $(document).ready(function () {

        $(function () {
            $('#DialogSlagTapping_Select').dialog({
                autoOpen: false,
                width: 550,
                height: 500,
                resizable: false,
                draggable: true,
                title: 'Add Slag Tapping',
                closeText: 'Close[x]',
                modal: true,
                position: [800, 260]
            });

            $('#DialogSlagTappingEdit_Select').dialog({
                autoOpen: false,
                width: 550,
                height: 500,
                resizable: false,
                draggable: true,
                title: 'Edit Slag Tapping',
                closeText: 'Close[x]',
                modal: true,
                position: [800, 260]
            });


            $('#DialogMatteTapping_Select').dialog({
                autoOpen: false,
                width: 650,
                height: 600,
                resizable: false,
                draggable: true,
                title: 'Add Matte Tapping',
                closeText: 'Close[x]',
                modal: true,
                position: [800, 260]
            });

            $('#DialogMatteTappingEdit_Select').dialog({
                autoOpen: false,
                width: 650,
                height: 600,
                resizable: false,
                draggable: true,
                title: 'Edit Matte Tapping',
                closeText: 'Close[x]',
                modal: true,
                position: [800, 260]
            });

        });

        load_fsfe_matte_tapping();
        load_fsfe_slag_tapping();


        $(".tabs-menu a").click(function (event) {
            event.preventDefault();
            $(this).parent().addClass("current");

            $(this).parent().siblings().removeClass("current");

            var tab = $(this).attr("href");
            $(".tab-content-pass").not(tab).css("display", "none");
            $(tab).fadeIn();
        });

    });

</script>
